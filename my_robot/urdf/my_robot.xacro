<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">


  <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />

  <!-- Joints -->
  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <joint name="camera_joint" type="fixed">
    <origin xyz="0.23 0 0.25" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="camera"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="camera_optical_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
    <parent link="camera"/>
    <child link="camera_link_optical"/>
  </joint>

  <joint name="hokuyo_joint" type="fixed">
    <origin xyz="0.15 0 0.32" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="hokuyo" />
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="cab_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="cab" />
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="front_right_wheel_hinge" type="continuous">
    <origin xyz="0.15 -0.15 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="front_right_wheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="15.0"/>
  </joint>

  <joint name="front_left_wheel_hinge" type="continuous">
    <origin xyz="0.15 0.15 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="front_left_wheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="15.0"/>
  </joint>

  <joint name="rear_right_wheel_hinge" type="continuous">
    <origin xyz="-0.15 -0.15 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="rear_right_wheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="15.0"/>
  </joint>

  <joint name="rear_left_wheel_hinge" type="continuous">
    <origin xyz="-0.15 0.15 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="rear_left_wheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="15.0"/>
  </joint>

  <joint name="arm_support_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="arm_support" />
  </joint>

  <joint name="left_arm_hinge" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="arm_support"/>
    <child link="left_arm"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit upper="0.52" lower="0" effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint name="right_arm_hinge" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="arm_support"/>
    <child link="right_arm"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit upper="0.52" lower="0" effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint name="right_bucket_hinge" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="right_arm"/>
    <child link="bucket" />
  </joint>

  <joint name="left_bucket_hinge" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="left_arm"/>
    <child link="bucket" />
  </joint>

  <!-- Links-->
  <link name="robot_footprint"></link>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size="0.4 0.2 0.1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.4 0.2 0.1"/>
      </geometry>
    </visual>
  </link>

  <link name='cab'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='cab_collision'>
      <origin xyz="0.08 0 0.15" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.3 0.18 0.3"/>
      </geometry>
    </collision>

    <visual name='cab_visual'>
      <origin xyz="0.08 0 0.15" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.3 0.18 0.3"/>
      </geometry>
    </visual>

  </link>

  <link name='arm_support'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
    <collision name='arm_support_collision'>
      <origin xyz="-0.15 0 0.15" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.18 0.2"/>
      </geometry>
    </collision>

    <visual name='arm_support_visual'>
      <origin xyz="-0.15 0 0.15" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.18 0.2"/> </geometry>
    </visual>

  </link>

  <link name='front_right_wheel'>

     <inertial>
          <origin xyz="0.0 0 0" rpy="0 0 0"/>
          <mass value="5.0"/>
          <inertia ixx="0.1" ixy="0" ixz="0"
                   iyy="0.1" iyz="0"
                   izz="0.1"/>
     </inertial>

    <collision name='front_right_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>

    <visual name='front_right_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>

  <link name='front_left_wheel'>

     <inertial>
          <origin xyz="0.0 0 0" rpy="0 0 0"/>
          <mass value="5.0"/>
          <inertia ixx="0.1" ixy="0" ixz="0"
                   iyy="0.1" iyz="0"
                   izz="0.1"/>
     </inertial>

    <collision name='front_left_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>

    <visual name='front_left_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>

  <link name='rear_right_wheel'>

     <inertial>
          <origin xyz="0.0 0 0" rpy="0 0 0"/>
          <mass value="5.0"/>
          <inertia ixx="0.1" ixy="0" ixz="0"
                   iyy="0.1" iyz="0"
                   izz="0.1"/>
     </inertial>

    <collision name='rear_right_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>

    <visual name='rear_right_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>

  <link name='rear_left_wheel'>

     <inertial>
          <origin xyz="0.0 0 0" rpy="0 0 0"/>
          <mass value="5.0"/>
          <inertia ixx="0.1" ixy="0" ixz="0"
                   iyy="0.1" iyz="0"
                   izz="0.1"/>
     </inertial>

    <collision name='rear_left_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>

    <visual name='rear_left_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>

  <link name='left_arm'>

     <inertial>
          <origin xyz="0.0 0 0" rpy="0 0 0"/>
          <mass value="5.0"/>
          <inertia ixx="0.1" ixy="0" ixz="0"
                   iyy="0.1" iyz="0"
                   izz="0.1"/>
     </inertial>

    <collision name='left_arm_collision'>
      <origin xyz="0.08 0.11 0.08" rpy="0 0.48 0"/>
      <geometry>
        <box size="0.58 0.01 0.04"/>
      </geometry>
    </collision>

    <visual name='left_arm_visual'>
      <origin xyz="0.08 0.11 0.08" rpy="0 0.48 0"/>
      <geometry>
        <box size="0.58 0.01 0.04"/>
      </geometry>
    </visual>

  </link>

  <link name='right_arm'>

     <inertial>
          <origin xyz="0.0 0 0" rpy="0 0 0"/>
          <mass value="5.0"/>
          <inertia ixx="0.1" ixy="0" ixz="0"
                   iyy="0.1" iyz="0"
                   izz="0.1"/>
     </inertial>

    <collision name='right_arm_collision'>
      <origin xyz="0.08 -0.11 0.08" rpy="0 0.48 0"/>
      <geometry>
        <box size="0.58 0.01 0.04"/>
      </geometry>
    </collision>

    <visual name='right_arm_visual'>
      <origin xyz="0.08 -0.11 0.08" rpy="0 0.48 0"/>
      <geometry>
        <box size="0.58 0.01 0.04"/>
      </geometry>
    </visual>
  </link>

  <link name='bucket'>

     <inertial>
          <origin xyz="0.0 0 0" rpy="0 0 0"/>
          <mass value="5.0"/>
          <inertia ixx="0.1" ixy="0" ixz="0"
                   iyy="0.1" iyz="0"
                   izz="0.1"/>
     </inertial>
    <collision name='bucket_collision'>
      <origin xyz="0.36 0.1 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.19 0.35 0.15"/>
      </geometry>
    </collision>

    <visual name='bucket_visual'>
      <origin xyz="0.36 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.19 0.35 0.15"/>
      </geometry>
    </visual>

  </link>

  <link name='camera'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.000001" ixy="0" ixz="0"
               iyy="0.000001" iyz="0"
               izz="0.000001"/>
    </inertial>

    <visual name='camera_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </visual>

    <collision name='camera_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>
  </link>

  <link name="camera_link_optical">
  </link>

  <link name='hokuyo'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="0.00001"/>
      <inertia ixx="0.000001" ixy="0" ixz="0"
               iyy="0.000001" iyz="0"
               izz="0.000001" />
    </inertial>

    <visual name='hokuyo_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>

    <collision name='hokuyo_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>
</robot>

